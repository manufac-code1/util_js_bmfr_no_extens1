{
  "cSpell.words": [
    "BMRN",
    "Boogerface",
    "buzzy",
    "mult",
    "vakata",
    "wholerow"
  ],
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/src/libs/themes"
    }
  ],
  "liveSassCompile.settings.includeItems": [
    "/src/libs/themes/**/*.less"
  ],
  "liveSassCompile.settings.excludeList": [
    "**/node_modules/**",
    ".vscode/**"
  ],
  "files.exclude": {
    "node_modules": true,
    "package-lock.json": true,
    "package.json": true,
    ".vscode": true,
    "public": true
  }
}